<RecursiveDock>

    <Preprocessor>
        <Random></Random>
    </Preprocessor>

    <Corridor>
        <Style>Wandering</Style>
        <Width>1</Width>
        <CentralConstant>
            <Entity>
                <Sprite>
                    <Name>Oryx/uf_split/uf_terrain/ground_grass_1</Name>
                </Sprite>
                <TilingSprite>
                    <Name>Path</Name>
                    <Mask>Masks/path</Mask>
                    <Sprite>
                        <Name>Oryx/uf_split/uf_terrain/ground_dirt_brown_1</Name>
                    </Sprite>
                </TilingSprite>
            </Entity>
            <Slot>Floor</Slot>
        </CentralConstant>
    </Corridor>

    <Generators>
        <CellularAutomata></CellularAutomata>
        <Starburst></Starburst>
        <Polygon></Polygon>
    </Generators>

    <Symbols>

        <Symbol>
            <Char>.</Char>
            <Floor>
                <Sprite>
                    <Name>Oryx/uf_split/uf_terrain/ground_grass_1</Name>
                </Sprite>
            </Floor>
        </Symbol>

        <Symbol Extends=".">
            <Char>#</Char>
            <Wall>
                <Sprite>
                    <Name>Oryx/Custom/terrain/tree1</Name>
                    <DrawActualSize>true</DrawActualSize>
                </Sprite>
            </Wall>
        </Symbol>

    </Symbols>

    <Rooms>

        <Filler Condition="6 + rnd(6)" />

        <Start>
            <Symbols>
                <Symbol Extends=".">
                    <Char>s</Char>
                    <Entity Extends="player"></Entity>
                </Symbol>
                <Symbol Extends=".">
                    <Char>e</Char>
                    <Entity Extends="enemy">
                        <Leader>player</Leader>
                    </Entity>
                </Symbol>
            </Symbols>
            <Rows>
                <Row>eee</Row>
                <Row>eee</Row>
                <Row>eee</Row>
                <Row>eee</Row>
                <Row>eee</Row>
                <Row>ese</Row>
                <Row>eee</Row>
            </Rows>
        </Start>

    </Rooms>

    <Theme>
        <Feature>
            <Placement>Hidden</Placement>
            <Coverage>0.6</Coverage>
            <Entity>
                <Sprite>
                    <Name>Oryx/uf_split/uf_terrain/ground_grass_3</Name>
                </Sprite>
            </Entity>
            <Slot>Floor</Slot>
            <Overwrite />
        </Feature>
    </Theme>

</RecursiveDock>