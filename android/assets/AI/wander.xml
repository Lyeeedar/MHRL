<DataScope>
	<Any>
		<Priority>
            <CheckValue Key="Pos"/>
			<Pick Criteria="rnd" Input="Tiles" Output="Pos"/>
		</Priority>
        <SetState Completed="Running" Failed="Completed">
            <Until State="Running">
                <MoveTo Key="Pos"/>
                <ClearValue Key="Pos"/>
            </Until>
        </SetState>
	</Any>
</DataScope>